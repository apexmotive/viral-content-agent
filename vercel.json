{
    "buildCommand": "cd frontend && npm run build",
    "devCommand": "cd frontend && npm run dev",
    "installCommand": "cd frontend && npm install && pip install -r requirements.txt",
    "framework": "nextjs",
    "outputDirectory": "frontend/.next",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.py"
        }
    ]
}